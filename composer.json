{
    "name": "castorius31/wrike-php-sdk",
    "description": "Wrike PHP SDK, configured Wrike PHP Library package for most user cases.",
    "keywords": ["wrike", "wrike.com", "api", "sdk", "library", "guzzle", "client", "REST", "HTTP", "php"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Zbigniew Ślązak"
        }
    ],
    "require": {
        "php": ">=8.0",
        "castorius31/wrike-php-library": "^1.0",
        "castorius31/wrike-php-guzzle": "^1.0",
        "zibios/wrike-php-jmsserializer": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=9.0"
    },
    "autoload": {
        "psr-4": {"Zibios\\WrikePhpSdk\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": { "Zibios\\WrikePhpSdk\\Tests\\": "tests/" }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/castorius31/wrike-php-library"
        },
        {
            "type": "vcs",
            "url": "https://github.com/castorius31/wrike-php-guzzle"
        }
      ]
}
